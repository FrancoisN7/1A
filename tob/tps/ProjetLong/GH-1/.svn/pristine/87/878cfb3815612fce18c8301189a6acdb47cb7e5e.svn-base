package deplacement;

import java.awt.event.*;

@SuppressWarnings("serial")
public class Grotte extends EcranAffiche{  

	public Grotte(){
		super();
	}
	
	@Override 
    public void mouseClicked(MouseEvent e) {
		ecran.dispose();
		ecran = new Carte();
        ecran.Afficher("carte.jpg");
    }  
	
}
